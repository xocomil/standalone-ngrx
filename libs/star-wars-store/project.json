{
  "name": "star-wars-store",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/star-wars-store/src",
  "prefix": "standalone-ngrx",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/star-wars-store/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/star-wars-store/**/*.ts",
          "libs/star-wars-store/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
